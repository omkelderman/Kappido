<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://www.han.nl/schemas/messages"
        xmlns:types="http://www.han.nl/schemas/types"
        elementFormDefault="qualified">

    <import namespace="http://www.han.nl/schemas/types" schemaLocation="types.xsd"/>

    <element name="MatchRequest">
        <complexType>
            <sequence>
                <element name="input" type="types:MatchInput" minOccurs="1" maxOccurs="1"/>
            </sequence>
        </complexType>
    </element>

    <element name="MatchResponse">
        <complexType>
            <sequence>
                <element name="result" type="types:MatchResult" minOccurs="1" maxOccurs="1"/>
            </sequence>
        </complexType>
    </element>
</schema>